<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
     "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
     "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
  <suppress checks="IndentationCheck"
             files=".*"/>
  <suppress checks="MethodNameCheck"
             files=".*"/>
  <suppress checks="MemberNameCheck"
             files=".*"/>
  <suppress checks="LocalVariableNameCheck"
             files=".*"/>
  <suppress checks="ParameterName"
             files=".*"/>
  <suppress checks="CustomImportOrder"
             files=".*"/>
  <suppress checks="NeedBracesCheck"
             files=".*"/>
</suppressions>
